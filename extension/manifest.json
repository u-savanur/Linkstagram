{
  "manifest_version": 3,
  "name": "Linkstagram Parser",
  "version": "1.0.0",
  "description": "Captures LinkedIn and Instagram profiles for analysis.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "http://localhost:3000/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Linkstagram"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
